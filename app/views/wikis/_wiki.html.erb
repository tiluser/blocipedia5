<div class="media">
    <div class="media-body">
        <h4 class="media-heading">
            <%= link_to wiki.title, user_wiki_path(wiki.user, wiki) %>
        </h4>
        <small>
            submitted <%= time_ago_in_words(wiki.created_at) %> ago by <%= wiki.user.name %>
        </small>
    </div>
</div>
